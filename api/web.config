<configuration>
	<system.webServer>
		<handlers>
			<add name="iisnode" path="./bin/www" verb="." modules="iisnode" />
		<handlers/>
		<rewrite>
			<rules>
				<rule name="sendToNode">
					<match url"/*" />
					<action type="Rewrite" url="./bin/www">
			</rules>
		</rewrite>
	</system.webServer>
</configuration>